list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")
find_package(ProtobufWithTargets)
find_package(gRPC)

include("${CMAKE_CURRENT_LIST_DIR}/googleapis-targets.cmake")

foreach (_target @googleapis_cpp_installed_libraries_list@ )
	
    string(REPLACE "googleapis_cpp_" "googleapis-c++::" scoped_name "${_target}" )
    set(imported_name "${_target}")
    if (NOT TARGET ${scoped_name})
        add_library(${scoped_name} IMPORTED INTERFACE)
        set_target_properties(${scoped_name}
                              PROPERTIES INTERFACE_LINK_LIBRARIES
                              ${imported_name})
    endif ()
endforeach ()
